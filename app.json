{
  "expo": {
    "name": "tkd-app",
    "slug": "tkd-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo2.png",
      "imageSize": {
        "width": 50,
        "height": 50
      },
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "animations": {
        "logo": {
          "width": ["80%", "60%"],
          "height": ["80%", "60%"],
          "translate": ["0%", "10%"],
          "rotate": ["0deg", "360deg"],
          "scale": [1, 0.8],
          "duration": 1000,
          "loop": true
        }
      }
    },
    "fontFamily": "./assets/fonts/roboto",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/logo2.png"
    }
  }
}
